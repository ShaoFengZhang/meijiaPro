.bg, #botScroll {
    background: #f6f6f8;
}

#botScroll {
    padding-top: 10rpx;
}

.orderListView {
    width: 748rpx;
    /* min-height: 500rpx;
    max-height: 544rpx; */
    box-sizing: border-box;
    padding: 0 38rpx 0 40rpx;
    background: white;
    margin-top: 16rpx;
    overflow: hidden;
}

.storename {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 40rpx;
}

.itemName, .itemTime, .itemCall, .itemAdress {
    color: #8e8e93;
    font-size: 32rpx;
    margin-top: 16rpx;
}

.itemAdress {
    width: 100%;
    /* height: 88rpx; */
}

.adressArea {
    width: 500rpx;
    min-height: 44rpx;
    max-height: 88rpx;
    display: -webkit-box;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    display: flex;
    align-items: center;
}

.line {
    width: 100%;
    height: 2rpx;
    background: #f6f6f8;
    margin-top: 32rpx;
}

.orderType {
    width: 100%;
    height: 118rpx;
    line-height: 118rpx;
    text-align: center;
}

.orderType view {
    width: 100%;
    height: 118rpx;
    font-size: 40rpx;
}

.noMyTempList{
    width: 100%;
    text-align: center;
    color: #888888;
    font-size: 28rpx;
}