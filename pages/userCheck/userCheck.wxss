.topPromt {
    width: 100%;
    height: 64rpx;
    font-size: 28rpx;
    color: #8e8e93;
    letter-spacing: -0.68rpx;
    background: #f6f6f8;
    line-height: 64rpx;
    box-sizing: border-box;
    padding: 0 32rpx;
    display: flex;
    justify-content: space-between;
}

.userList {
    width: 100%;
    height: 142rpx;
    background: #fff;
    box-sizing: border-box;
    padding: 32rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
}

.userIcon, .userIcon image {
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    margin-right: 32rpx;
}

.userName {
    width: 320rpx;
    height: 142rpx;
    line-height: 140rpx;
    font-size: 30rpx;
    color: #262628;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.checkDetails {
    width:300rpx;
    height: 70rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 20rpx;
}

.checkDetails {
    font-size: 24rpx;
    color: #666;
    text-align: right;
}

.line{
    width: 718rpx;
    height: 2rpx;
    position: absolute;
    right: 0;
    bottom: 0;
    background: #EFEFEF;
}

.noMyTempList{
    width: 100%;
    text-align: center;
    color: #888888;
    font-size: 28rpx;
}